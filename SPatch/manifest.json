{
    "manifest_version": 3,
    "name": "Saleswarp Patch",
    "short_name": "Patch",
    "version": "1.0.0",
    "description": "Patch for saleswarp website",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.website.com/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "host_permissions": [
        "https://www.website.com/"
    ]
}
